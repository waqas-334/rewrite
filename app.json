{
  "expo": {
    "name": "AI Rewrite",
    "slug": "grammar",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FAF6FF",
          "image": "./assets/splash-icon.png",
          "dark": {
            "image": "./assets/splash-icon.png",
            "backgroundColor": "#FAF6FF"
          },
          "imageWidth": 128,
          "imageHeight": 128
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "backgroundColor": "#FAF6FF",
      "imageWidth": 128,
      "imageHeight": 128,
      "dark": {
        "image": "./assets/splash-icon.png",
        "backgroundColor": "#FAF6FF"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deploy.grammarai",
      "buildNumber": "1.1.3",
      "icon": "./assets/icon.png",
      "appStoreUrl": "https://apps.apple.com/us/app/ai-rewrite-spell-checker/id6739363989"
    },
    "macOS": {
      "supportsM1": true,
      "bundleIdentifier": "com.deploy.grammarai"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deploy.grammarai"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "166fa6ee-b94e-45cd-8185-eddcd29dfd48"
      }
    },
    "owner": "data2004"
  }
}
